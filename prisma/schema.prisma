// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum UserRole {
  CO
  SL
}

enum StatusDo {
  Draft
  Submitted
  Processed
  Released
  Rejected
}

model Role {
  id   Int      @id @default(autoincrement())
  name UserRole @unique

  users          User[]
  requestor_form td_do_requestor_form[]
}

model User {
  id     Int     @id @default(autoincrement())
  roleId Int
  name   String  @unique
  hash   String?

  role    Role                   @relation(fields: [roleId], references: [id])
  reqdo   td_reqdo_header_form[]
  depo    td_depo[]
  session Session[]
}

model Session {
  id           Int    @id @default(autoincrement())
  accessToken  String
  refreshToken String

  user   User @relation(fields: [userId], references: [id])
  userId Int
}

// HEADER FORM
model td_reqdo_header_form {
  id           Int      @id @default(autoincrement())
  no_reqdo     String   @unique @default(dbgenerated("CONCAT('LNSW', EXTRACT(EPOCH FROM NOW())::Int)::text"))
  tgl_reqdo    DateTime @default(now())
  request_type Int
  order_id     String   @unique
  timezone     String?
  created_at   DateTime @default(now())
  created_by   String   @db.VarChar(255)

  author                  User                      @relation(fields: [created_by], references: [name])
  td_do_req_form          td_do_req_form?
  td_parties_detail_form  td_parties_detail_form?
  td_do_requestor_form    td_do_requestor_form?
  td_do_bl_form           td_do_bl_form?
  td_do_invoice_form      td_do_invoice_form[]
  td_do_dok_form          td_do_dok_form[]
  td_reqdo_status         td_reqdo_status[]
  td_do_kontainer_form    td_do_kontainer_form[]
  td_do_nonkontainer_form td_do_nonkontainer_form[]
}

// REQUESTOR FORM
model td_do_requestor_form {
  id                  Int      @id @default(autoincrement())
  id_reqdo_header     Int      @unique
  id_jenis_requestor  Int
  nama                String   @db.VarChar(255)
  nib                 String   @db.VarChar(15)
  npwp                String   @db.VarChar(16)
  alamat              String   @db.VarChar(255)
  filepath_suratkuasa String?
  created_at          DateTime @default(now())
  created_by          String

  do_req_header td_reqdo_header_form @relation(fields: [id_reqdo_header], references: [id], onDelete: Cascade)
  Role          Role?                @relation(fields: [roleId], references: [id])
  roleId        Int?
}

// DO REQ FORM
model td_do_req_form {
  id              Int       @id @default(autoincrement())
  id_reqdo_header Int       @unique
  id_terminal_op  String?
  id_shippingline String
  id_metode_bayar Int
  nama_vessel     String    @db.VarChar(45)
  no_voyage       String    @db.VarChar(20)
  no_bc11         String?   @db.VarChar(20)
  tanggal_bc11    DateTime?
  pos_number      String?   @db.VarChar(45)
  call_sign       String?   @db.VarChar(10)
  no_do_release   String?   @db.VarChar(50)
  tgl_reqdo_exp   DateTime
  tgl_do_exp      DateTime?
  tgl_do_release  DateTime?
  updated_by      String?
  updated_at      DateTime  @updatedAt
  created_at      DateTime  @default(now())
  created_by      String

  do_req_header td_reqdo_header_form @relation(fields: [id_reqdo_header], references: [id], onDelete: Cascade)
}

// PARTIES DETAIL FORM
model td_parties_detail_form {
  id                  Int      @id @default(autoincrement())
  id_reqdo_header     Int      @unique
  id_negara_loading   String
  id_port_loading     String
  id_port_destination String
  id_port_discharge   String
  nama_shipper        String   @db.VarChar(255)
  nama_consignee      String   @db.VarChar(255)
  npwp_consignee      String   @db.VarChar(16)
  nama_notifyparty    String   @db.VarChar(255)
  npwp_notifyparty    String   @db.VarChar(16)
  updated_by          String?  @db.VarChar(255)
  updated_at          DateTime @updatedAt
  created_at          DateTime @default(now())
  created_by          String

  do_req_header td_reqdo_header_form @relation(fields: [id_reqdo_header], references: [id], onDelete: Cascade)
}

// KONTAINER FORM
model td_do_kontainer_form {
  id                   Int      @id @default(autoincrement())
  id_reqdo_header      Int
  id_depo              Int?
  id_sizeType          String
  id_ownership         Int
  id_gross_weight_unit String
  no_kontainer         String   @db.VarChar(20)
  gross_weight         Float
  updated_by           String?  @db.VarChar(255)
  updated_at           DateTime @updatedAt
  created_at           DateTime @default(now())
  created_by           String
  td_depoId            Int?

  seals         sealsOnKontainers[]
  do_req_header td_reqdo_header_form @relation(fields: [id_reqdo_header], references: [id], onDelete: Cascade)
  td_depo       td_depo?             @relation(fields: [td_depoId], references: [id])
}

model sealsOnKontainers {
  kontainer   td_do_kontainer_form @relation(fields: [kontainerId], references: [id], onDelete: Cascade)
  kontainerId Int // relation scalar field (used in the `@relation` attribute above)
  seal        td_do_kontainer_seal @relation(fields: [sealId], references: [id], onDelete: Cascade)
  sealId      Int // relation scalar field (used in the `@relation` attribute above)
  assignedAt  DateTime             @default(now())
  assignedBy  String

  @@id([kontainerId, sealId])
}

// DEPO
model td_depo {
  id         Int    @id @default(autoincrement())
  id_kabkota String
  created_by String @db.VarChar(255)
  npwp       String @db.VarChar(15)
  deskripsi  String @db.VarChar(255)
  alamat     String @db.VarChar(1025)
  kode_pos   String @db.VarChar(5)
  no_telp    String @db.VarChar(20)

  author            User                   @relation(fields: [created_by], references: [name], onDelete: Cascade)
  do_kontainer_item td_do_kontainer_form[]
}

// KONTAINER SEAL
model td_do_kontainer_seal {
  id      Int    @id @default(autoincrement())
  no_seal String

  kontainers sealsOnKontainers[]
}

// NON-KONTAINER FORM
model td_do_nonkontainer_form {
  id                   Int      @id @default(autoincrement())
  id_reqdo_header      Int
  id_package_unit      String
  id_gross_weight_unit String
  good_desc            String
  package_qty          Float
  gross_weight         Float
  measurement_vol      Float
  measurement_unit     String   @db.VarChar(5)
  updated_by           String?  @db.VarChar(255)
  updated_at           DateTime @updatedAt
  created_at           DateTime @default(now())
  created_by           String

  do_req_header td_reqdo_header_form @relation(fields: [id_reqdo_header], references: [id], onDelete: Cascade)
}

// BL FORM
model td_do_bl_form {
  id              Int      @id @default(autoincrement())
  id_reqdo_header Int      @unique
  id_jenis_bl     String
  no_bl           String   @db.VarChar(50)
  tgl_bl          DateTime
  filepath_dok    String
  updated_by      String?  @db.VarChar(255)
  updated_at      DateTime @updatedAt
  created_at      DateTime @default(now())
  created_by      String

  do_req_header td_reqdo_header_form @relation(fields: [id_reqdo_header], references: [id], onDelete: Cascade)
  do_vin        td_do_vin[]
}

// VIN FORM
model td_do_vin {
  id     Int            @id @default(autoincrement())
  id_bl  Int
  no_vin String         @db.VarChar(50)
  do_bl  td_do_bl_form? @relation(fields: [id_bl], references: [id], onDelete: Cascade)
}

// INVOICE FORM
model td_do_invoice_form {
  id                  Int      @id @default(autoincrement())
  id_reqdo_header     Int
  id_currency         String
  id_bank             String
  no_invoice          String   @db.VarChar(255)
  tgl_invoice         DateTime
  total_payment       Int
  no_rekening         String   @db.VarChar(30)
  filepath_buktibayar String
  updated_by          String?  @db.VarChar(255)
  updated_at          DateTime @updatedAt
  created_at          DateTime @default(now())
  created_by          String

  do_req_header td_reqdo_header_form @relation(fields: [id_reqdo_header], references: [id], onDelete: Cascade)
}

// SUPPORT DOCUMENT FORM
model td_do_dok_form {
  id              Int      @id @default(autoincrement())
  id_reqdo_header Int
  id_jenis_dok    String
  no_dok          String   @db.VarChar(50)
  tgl_dok         DateTime
  filepath_dok    String
  updated_by      String?  @db.VarChar(255)
  updated_at      DateTime @updatedAt
  created_at      DateTime @default(now())
  created_by      String   @db.VarChar(255)

  do_req_header td_reqdo_header_form @relation(fields: [id_reqdo_header], references: [id], onDelete: Cascade)
}

// REQ DO STATUS
model td_reqdo_status {
  id              Int      @id @default(autoincrement())
  id_reqdo_header Int
  name            StatusDo
  datetime_status DateTime @default(now())
  note            String?

  do_req_header td_reqdo_header_form @relation(fields: [id_reqdo_header], references: [id], onDelete: Cascade)
}
